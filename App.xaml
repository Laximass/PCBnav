<Application x:Class="PCB_Nav3.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:properties="clr-namespace:PCB_Nav3.Properties"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <!-- Global resource definitions -->
        <SolidColorBrush x:Key="BGColour" Color="{x:Static properties:Settings.Default.BGC1}" />
        <SolidColorBrush x:Key="TextColour" Color="{x:Static properties:Settings.Default.TXTC2}" />
    </Application.Resources>
</Application>
